<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="circle" class="com.woslx.spring.beans.Circle">
        <property name="name" value="circle"/>
    </bean>

    <bean id="c2" class="com.woslx.spring.beans.Circle">
        <property name="name" value="c2"/>
    </bean>
    <bean id="rectangle" class="com.woslx.spring.beans.Rectangle"/>

    <!--shapeBox  -->
    <bean id="shapeBox" class="com.woslx.spring.beans.ShapeBox"/>

</beans>